{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geektrust",
  "version": "1.0.0",
  "type": "module",
  "description": "geektrust-project",
  "main": "geektrust.js",
  "scripts": {
    "install": "npm install",
    "start": "node geektrust.js",
    "start:sample": "node geektrust.js sample_input/input1.txt",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage"
  },
  "jest": {
    "preset": "default",
    "extensionsToTreatAsEsm": [
      ".js"
    ],
    "transform": {},
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "geektrust.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "author": "geektrust",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
